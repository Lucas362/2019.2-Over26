<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.eps.creditoffer">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:usesCleartextTraffic="true"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        >
        <activity android:name=".activities.TimeTravel"></activity>
        <activity android:name=".activities.MainActivity"/>
        <activity android:name=".activities.CashIn"/>
        <activity android:name=".activities.CashOut"/>
        <activity android:name=".activities.TrackLimit"/>
        <activity android:name=".activities.Eligibility"/>
        <activity android:name=".activities.Installment"/>
        <activity android:name=".activities.OverdraftConfirmation"/>
        <activity android:name=".activities.StartActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>